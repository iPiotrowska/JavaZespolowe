<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="rootPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ControllerMenu">
   <children>
      <Button fx:id="addPerson" mnemonicParsing="false" onAction="#addPerson" prefHeight="100.0" prefWidth="200.0" text="Dodaj osobę" />
      <Button fx:id="addBook" layoutX="200.0" mnemonicParsing="false" onAction="#addBook" prefHeight="100.0" prefWidth="200.0" text="Dodaj książkę" />
      <Button fx:id="peopleList" layoutY="100.0" mnemonicParsing="false" onAction="#peopleList" prefHeight="100.0" prefWidth="200.0" text="Spis osób" />
      <Button fx:id="bookList" layoutX="200.0" layoutY="100.0" mnemonicParsing="false" onAction="#bookList" prefHeight="100.0" prefWidth="200.0" text="Spis książek" />
      <Button fx:id="borrowBook" layoutY="200.0" mnemonicParsing="false" onAction="#borrowBook" prefHeight="100.0" prefWidth="200.0" text="Wypożycz" />
   </children>
</AnchorPane>
